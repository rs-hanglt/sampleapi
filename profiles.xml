<?xml version="1.0" encoding="UTF-8"?>
<profiles>
  <profile>
    <id>release</id>
    <activation>
      <activeByDefault>false</activeByDefault>
    </activation>
    <properties>
      <maven.test.skip>true</maven.test.skip>
      <war-plugin-web.xml>${basedir}/target/jspweb.xml</war-plugin-web.xml>
      <war-plugin-warSourceExcludes>**/*.jsp</war-plugin-warSourceExcludes>
    </properties>
  </profile>
  <profile>
    <id>server</id>
    <activation>
      <activeByDefault>false</activeByDefault>
    </activation>
    <properties>
      <locales>ja</locales>
      <maven.compiler.encoding>UTF-8</maven.compiler.encoding>
      <encoding>UTF-8</encoding>
      <docencoding>UTF-8</docencoding>
      <charset>UTF-8</charset>
      <jxr-inputEncoding>UTF-8</jxr-inputEncoding>
      <jxr-outputEncoding>UTF-8</jxr-outputEncoding>
      <inputEncoding>UTF-8</inputEncoding>
      <outputEncoding>UTF-8</outputEncoding>
    </properties>
  </profile>
  <profile>
    <id>synergy-options</id>
    <activation>
      <activeByDefault>true</activeByDefault>
    </activation>
    <properties>
      <synergy-home-url>http://circle.gmo-research.jp/trac/synergy</synergy-home-url>
      <synergy-scm-url>https://github.com/gr-terasawa/sampleapi.git</synergy-scm-url>
      <synergy-core-version>1.1.0</synergy-core-version>
      <synergy-tools-version>1.3.4</synergy-tools-version>
      <synergy-test-version>0.3.9</synergy-test-version>
      <synergy-repository-url>http://circle.gmo-research.jp/synergy</synergy-repository-url>
      <synergy-site-url>dav:http://circle.gmo-research.jp/synergy/site/sampleapi</synergy-site-url>
      <synergy-checkStyle-name>synergy Checks</synergy-checkStyle-name>
    </properties>
  </profile>
  <profile>
    <id>dependency-options</id>
    <activation>
      <activeByDefault>true</activeByDefault>
    </activation>
    <properties>
      <maven-clover2-plugin-version>2.4.3</maven-clover2-plugin-version>
    </properties>
  </profile>
  <profile>
    <id>clover-instrument</id>
    <activation>
      <activeByDefault>false</activeByDefault>
    </activation>
    <properties>
      <maven.clover.encoding>MS932</maven.clover.encoding>
      <maven.test.skip>true</maven.test.skip>
      <war-plugin-web.xml>${basedir}/src/main/webapp/WEB-INF/web.xml</war-plugin-web.xml>
      <war-plugin-warSourceExcludes />
    </properties>
  </profile>
</profiles>
