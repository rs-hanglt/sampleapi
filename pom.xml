  <build>
    <finalName>${artifactId}</finalName>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-eclipse-plugin</artifactId>
        <configuration>
          <additionalProjectnatures>
            <projectnature>
              org.springframework.ide.eclipse.core.springnature
            </projectnature>
            <projectnature>
              com.atlassw.tools.eclipse.checkstyle.CheckstyleNature
            </projectnature>
          </additionalProjectnatures>
          <additionalBuildcommands>
            <buildcommand>
              com.atlassw.tools.eclipse.checkstyle.CheckstyleBuilder
            </buildcommand>
          </additionalBuildcommands>
          <additionalConfig>
            <file>
              <name>.checkstyle</name>
              <content>
&lt;?xml version="1.0" encoding="UTF-8"?&gt;
&lt;fileset-config file-format-version="1.2.0" simple-config="true"&gt;
    &lt;fileset name="all" enabled="true" check-config-name="${synergy-checkStyle-name}" local="false"&gt;
        &lt;file-match-pattern match-pattern="." include-pattern="true"/&gt;
    &lt;/fileset&gt;
&lt;/fileset-config&gt;
              </content>
            </file>
          </additionalConfig>
          <downloadSources>true</downloadSources>
          <downloadJavadocs>true</downloadJavadocs>
          <wtpversion>1.5</wtpversion>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-surefire-plugin</artifactId>
        <configuration>
          <forkMode>never</forkMode>
          <argLine>-enableassertions</argLine>
        </configuration>
      </plugin>
      <plugin>
        <groupId>com.atlassian.maven.plugins</groupId>
        <artifactId>maven-clover2-plugin</artifactId>
        <version>2.4.3</version>
        <configuration>
          <jdk>1.6</jdk>
          <generatePdf>true</generatePdf>
          <generateXml>true</generateXml>
          <generateHistorical>true</generateHistorical>
          <cloverDatabase>${user.home}/.m2/clover/${artifactId}/clover.db</cloverDatabase>
          <cloverMergeDatabase>${user.home}/.m2/clover/${artifactId}/cloverMerge.db</cloverMergeDatabase>
          <historyDir>${user.home}/.m2/clover/${artifactId}/history</historyDir>
          <licenseLocation>${user.home}/.m2/clover.license</licenseLocation>
          <includesTestSourceRoots>true</includesTestSourceRoots>
          <flushPolicy>threaded</flushPolicy>
          <flushInterval>5000</flushInterval>
          <excludes>
            <exclude>**/*Dummy.java</exclude>
            <exclude>**/*Facade*.java</exclude>
          </excludes>
        </configuration>
        <executions>
          <execution>
            <phase>pre-site</phase>
            <goals>
              <goal>instrument</goal>
              <goal>save-history</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.6</source>
          <target>1.6</target>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-war-plugin</artifactId>
        <configuration>
          <webXml>${war-plugin-web.xml}</webXml>
          <warSourceExcludes>${war-plugin-warSourceExcludes}</warSourceExcludes>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-dependency-plugin</artifactId>
        <configuration>
          <outputDirectory>
            ${basedir}/src/main/webapp/WEB-INF/lib
          </outputDirectory>
          <excludeScope>provided</excludeScope>
          <includeScope>compile</includeScope>
          <excludeArtifactIds>jsp-api,servlet-api</excludeArtifactIds>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-site-plugin</artifactId>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-checkstyle-plugin</artifactId>
        <configuration>
          <configLocation>${basedir}/config/checkstyle-configuration.xml</configLocation>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-javadoc-plugin</artifactId>
        <version>2.3</version>
        <configuration>
          <links>
            <link>http://docs.oracle.com/javase/jp/6/api/</link>
          </links>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-release-plugin</artifactId>
        <version>2.0-beta-9</version>
        <configuration>
          <preparationGoals>jspc:compile compile</preparationGoals>
        </configuration>
      </plugin>
    </plugins>
    <extensions>
      <extension>
        <groupId>org.apache.maven.wagon</groupId>
        <artifactId>wagon-webdav</artifactId>
        <!--version>1.0-beta-2</version-->
      </extension>
    </extensions>
  </build>
  <scm>
    <connection>scm:git:git@github.com:gr-terasawa/git-demo.git</connection>
    <url>scm:git:git@github.com:gr-terasawa/git-demo.git</url>
    <developerConnection>scm:git:git@github.com:gr-terasawa/git-demo.git</developerConnection>
  </scm>
  <repositories>
    <repository>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
      <id>synergy</id>
      <name>Maven Repository Switchboard</name>
      <url>http://circle.gmo-research.jp/synergy</url>
      <releases>
        <checksumPolicy>warn</checksumPolicy>
      </releases>
    </repository>
  </repositories>

  <distributionManagement>
    <site>
      <id>synergy</id>
      <url>dav:http://circle.gmo-research.jp/synergy/site/sampleapi</url>
    </site>
  </distributionManagement>
  <reporting>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-javadoc-plugin</artifactId>
        <version>2.3</version>
        <reportSets>
          <reportSet>
            <id>html</id>
            <configuration>
              <links>
                <link>http://docs.oracle.com/javase/jp/6/api/</link>
              </links>
            </configuration>
            <reports>
              <report>javadoc</report>
            </reports>
          </reportSet>
          <reportSet>
            <id>test-html</id>
            <configuration>
              <links>
                <link>http://docs.oracle.com/javase/jp/6/api/</link>
              </links>
            </configuration>
            <reports>
              <report>test-javadoc</report>
            </reports>
          </reportSet>
        </reportSets>
      </plugin>
      <plugin>
        <groupId>com.atlassian.maven.plugins</groupId>
        <artifactId>maven-clover2-plugin</artifactId>
        <version>2.4.3</version>
        <configuration>
          <jdk>1.6</jdk>
          <generatePdf>true</generatePdf>
          <generateHistorical>true</generateHistorical>
          <cloverDatabase>${user.home}/.m2/clover/${artifactId}/clover.db</cloverDatabase>
          <cloverMergeDatabase>${user.home}/.m2/clover/${artifactId}/cloverMerge.db</cloverMergeDatabase>
          <historyDir>${user.home}/.m2/clover/${artifactId}/history</historyDir>
          <licenseLocation>${user.home}/.m2/clover.license</licenseLocation>
          <includesTestSourceRoots>true</includesTestSourceRoots>
          <excludes>
            <exclude>**/*Dummy.java</exclude>
            <exclude>**/*Facade*.java</exclude>
          </excludes>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-pmd-plugin</artifactId>
        <configuration>
          <linkXref>true</linkXref>
          <minimumTokens>100</minimumTokens>
          <targetJdk>1.6</targetJdk>
          <skip>false</skip>
          <excludes>
            <exclude>**/*Dto.java</exclude>
          </excludes>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-checkstyle-plugin</artifactId>
        <configuration>
          <configLocation>${basedir}/config/checkstyle-configuration.xml</configLocation>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>taglist-maven-plugin</artifactId>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-jxr-plugin</artifactId>
        <configuration>
          <inputEncoding>${jxr-inputEncoding}</inputEncoding>
          <outputEncoding>${jxr-outputEncoding}</outputEncoding>
        </configuration>
      </plugin>
    </plugins>
  </reporting>
  <properties>
    <maven.compiler.encoding>MS932</maven.compiler.encoding>
    <war-plugin-web.xml>${basedir}/src/main/webapp/WEB-INF/web.xml</war-plugin-web.xml>
    <maxmemory>256M</maxmemory>
    <additionalparam>-keywords</additionalparam>
    <encoding>MS932</encoding>
    <docencoding>MS932</docencoding>
    <charset>MS932</charset>
    <javadocVersion>1.6</javadocVersion>
    <overview>${basedir}/src/main/javadoc/overview.html</overview>
    <bottom><![CDATA[Copyright &#169; {inceptionYear}-{currentYear} <a href="${organization.url}">{organizationName}</a>. All Rights Reserved.]]></bottom>
    <javadocVersion>1.6</javadocVersion>
    <linksource>true</linksource>
    <jxr-inputEncoding>MS932</jxr-inputEncoding>
    <jxr-outputEncoding>MS932</jxr-outputEncoding>
    <locales>ja</locales>
    <inputEncoding>MS932</inputEncoding>
    <outputEncoding>MS932</outputEncoding>
    <war-plugin-warSourceExcludes />
  </properties>

<profiles>
<profile>
<id>release</id>
<activation>
<activeByDefault>false</activeByDefault>
</activation>
