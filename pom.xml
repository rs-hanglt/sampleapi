<profile>
<id>synergy-options</id>
<activation>
<activeByDefault>true</activeByDefault>
</activation>
<properties>
<synergy-home-url>http://circle.gmo-research.jp/trac/synergy</synergy-home-url>
<synergy-scm-url>https://github.com/gr-terasawa/sampleapi.git</synergy-scm-url>
<synergy-core-version>1.1.0</synergy-core-version>
<synergy-tools-version>1.3.4</synergy-tools-version>
<synergy-test-version>0.3.9</synergy-test-version>
