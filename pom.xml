</profile>
<profile>
<id>dependency-options</id>
<activation>
<activeByDefault>true</activeByDefault>
</activation>
<properties>
<maven-clover2-plugin-version>2.4.3</maven-clover2-plugin-version>
</properties>
</profile>
<profile>
<id>clover-instrument</id>
<activation>
<activeByDefault>false</activeByDefault>
</activation>
<properties>
